{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "import"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/no-danger": "off",
    "camelcase": "off",
    "max-len": [
      "warn",
      {
        "code": 150
      }
    ],
    "no-console": "off",
    "jsx-a11y/anchor-is-valid": "off"
  }
}